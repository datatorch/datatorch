{
  "name": "datatorch",
  "private": true,
  "version": "0.0.0",
  "workspaces": [
    "shared/js/*",
    "clients/*",
    "services/*",
    "docs/*",
    "tests/*"
  ],
  "scripts": {
    "dev": "yarn install && yarn dev:services",
    "dev:services": "",
    "dev:web": "",
    "dev:cli": "go run ./clients/cli",
    "build": "",
    "lint": "yarn lint:go && yarn lint:python && yarn lint:js && yarn lint:terraform",
    "lint:go": "golangci-lint run",
    "lint:python": "python -m black .",
    "lint:terraform": "terraform fmt -recursive",
    "lint:terraform:ci": "terraform fmt -recursive -check",
    "lint:js": "eslint --ext .tsx,.ts,.jsx,.js --ignore-path .gitignore . --fix",
    "start": "",
    "ext:up": "docker-compose -f docker-compose.dev.yaml up -d",
    "ext:down": "docker-compose -f docker-compose.dev.yaml down",
    "test": ""
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.17.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^5.1.3",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3"
  }
}
